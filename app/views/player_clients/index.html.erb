<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing player_clients</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Player</th>

    <th>Client</th>

    <th></th>
  </tr>

<% @player_clients.each do |player_client| %>
  <tr>

    <td><%= player_client.player_id %></td>

    <td><%= player_client.client_id %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), player_client, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_player_client_path(player_client), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), player_client, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_player_client_path, :title=>"Add a player_client" %>
<% end %>
<%= content_for :footer do %>
<% end %>

