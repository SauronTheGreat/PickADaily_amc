<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing client_earnings</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Round</th>

    <th>Player</th>

    <th>Client</th>

    <th>Earning</th>

    <th>Fee payout</th>

    <th></th>
  </tr>

<% @client_earnings.each do |client_earning| %>
  <tr>

    <td><%= client_earning.round_id %></td>

    <td><%= client_earning.player_id %></td>

    <td><%= client_earning.client_id %></td>

    <td><%= client_earning.earning %></td>

    <td><%= client_earning.fee_payout %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), client_earning, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_client_earning_path(client_earning), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), client_earning, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_client_earning_path, :title=>"Add a client_earning" %>
<% end %>
<%= content_for :footer do %>
<% end %>

