<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing imports</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Name</th>

    <th></th>
  </tr>

<% @imports.each do |import| %>
  <tr>

    <td><%= import.name %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), import, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_import_path(import), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), import, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_import_path, :title=>"Add a import" %>
<% end %>
<%= content_for :footer do %>
<% end %>

