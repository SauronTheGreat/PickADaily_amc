<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing student_group_users</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Student group</th>

    <th>User</th>

    <th></th>
  </tr>

<% @student_group_users.each do |student_group_user| %>
  <tr>

    <td><%= student_group_user.student_group_id %></td>

    <td><%= student_group_user.user_id %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), student_group_user, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_student_group_user_path(student_group_user), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), student_group_user, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_student_group_user_path, :title=>"Add a student_group_user" %>
<% end %>
<%= content_for :footer do %>
<% end %>

