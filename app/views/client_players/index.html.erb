<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing client_players</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Client</th>

    <th>Round</th>

    <th>Player</th>

    <th>Amount</th>

    <th></th>
  </tr>

<% @client_players.each do |client_player| %>
  <tr>

    <td><%= client_player.client_id %></td>

    <td><%= client_player.round_id %></td>

    <td><%= client_player.player_id %></td>

    <td><%= client_player.amount %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), client_player, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_client_player_path(client_player), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), client_player, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_client_player_path, :title=>"Add a client_player" %>
<% end %>
<%= content_for :footer do %>
<% end %>

