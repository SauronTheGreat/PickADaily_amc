<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing player_earnings</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Round</th>

    <th>Player</th>

    <th>Commissions</th>

    <th>Fine</th>

    <th>Income</th>

    <th></th>
  </tr>

<% @player_earnings.each do |player_earning| %>
  <tr>

    <td><%= player_earning.round_id %></td>

    <td><%= player_earning.player_id %></td>

    <td><%= player_earning.commissions %></td>

    <td><%= player_earning.fine %></td>

    <td><%= player_earning.income %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), player_earning, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_player_earning_path(player_earning), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), player_earning, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_player_earning_path, :title=>"Add a player_earning" %>
<% end %>
<%= content_for :footer do %>
<% end %>

