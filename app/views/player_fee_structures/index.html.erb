<%= content_for :head do %>
    <script type="text/javascript">
        $(function() {
        });
    </script>
<% end %>

<%= content_for :header do %>
    <h1>Listing player_fee_structures</h1>
<% end %>

<%= content_for :main_content do %>
    <table>
      <tr>

        <th>Round</th>

        <th>Player</th>

        <th>Management fee</th>

        <th>Carry fee</th>

        <th></th>
      </tr>

      <% @player_fee_structures.each do |player_fee_structure| %>
          <tr>

            <td><%= player_fee_structure.round_id %></td>

            <td><%= player_fee_structure.player_id %></td>

            <td><%= player_fee_structure.management_fee %></td>

            <td><%= player_fee_structure.carry_fee %></td>

            <td>
              <%= link_to image_tag(icon("show"), :size=>"64x64"), player_fee_structure, :title=>'Show' %>
              <%= link_to image_tag(icon("edit"), :size=>"64x64"), edit_player_fee_structure_path(player_fee_structure,:player_id=>@player.id), :title=>'Edit' %>

            </td>
          </tr>
      <% end %>
    </table>
<% end %>

<br/>
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
    <% if @player_fee_structures.count==0 %>
        <%= link_to image_tag('new_icon.png', :size=>"100x100"), new_player_fee_structure_path(:player_id=>@player.id), :title=>"Add a player_fee_structure" %>
    <% end %>
<% end %>
<%= content_for :footer do %>
<% end %>

