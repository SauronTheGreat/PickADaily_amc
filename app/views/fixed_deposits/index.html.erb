<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing fixed_deposits</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Company</th>

    <th>Rate of interest</th>

    <th>Tenure</th>

    <th></th>
  </tr>

<% @fixed_deposits.each do |fixed_deposit| %>
  <tr>

    <td><%= fixed_deposit.company_id %></td>

    <td><%= fixed_deposit.rate_of_interest %></td>

    <td><%= fixed_deposit.tenure %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), fixed_deposit, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_fixed_deposit_path(fixed_deposit), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), fixed_deposit, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_fixed_deposit_path, :title=>"Add a fixed_deposit" %>
<% end %>
<%= content_for :footer do %>
<% end %>

