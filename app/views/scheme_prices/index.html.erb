<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing scheme_prices</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Round</th>

    <th>Scheme</th>

    <th>Price date</th>

    <th>Price</th>

    <th></th>
  </tr>

<% @scheme_prices.each do |scheme_price| %>
  <tr>

    <td><%= scheme_price.round_id %></td>

    <td><%= scheme_price.scheme_id %></td>

    <td><%= scheme_price.price_date %></td>

    <td><%= scheme_price.price %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), scheme_price, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_scheme_price_path(scheme_price), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), scheme_price, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_scheme_price_path, :title=>"Add a scheme_price" %>
<% end %>
<%= content_for :footer do %>
<% end %>

