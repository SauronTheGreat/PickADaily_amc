<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing round_stock_constants</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Constant</th>

    <th>Round</th>

    <th>Month</th>

    <th>Amount</th>

    <th></th>
  </tr>

<% @round_stock_constants.each do |round_stock_constant| %>
  <tr>

    <td><%= round_stock_constant.constant_id %></td>

    <td><%= round_stock_constant.round_id %></td>

    <td><%= round_stock_constant.month_id %></td>

    <td><%= round_stock_constant.amount %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), round_stock_constant, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_round_stock_constant_path(round_stock_constant), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), round_stock_constant, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_round_stock_constant_path, :title=>"Add a round_stock_constant" %>
<% end %>
<%= content_for :footer do %>
<% end %>

