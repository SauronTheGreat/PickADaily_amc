<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing facilitators</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>User</th>

    <th></th>
  </tr>

<% @facilitators.each do |facilitator| %>
  <tr>

    <td><%= facilitator.user_id %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), facilitator, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_facilitator_path(facilitator), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), facilitator, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_facilitator_path, :title=>"Add a facilitator" %>
<% end %>
<%= content_for :footer do %>
<% end %>

