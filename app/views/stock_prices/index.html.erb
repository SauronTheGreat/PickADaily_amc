<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing stock_prices</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Round</th>

    <th>Stock</th>

    <th>Open</th>

    <th>High</th>

    <th>Low</th>

    <th>Close</th>

    <th></th>
  </tr>

<% @stock_prices.each do |stock_price| %>
  <tr>

    <td><%= stock_price.round_id %></td>

    <td><%= stock_price.stock_id %></td>

    <td><%= stock_price.open %></td>

    <td><%= stock_price.high %></td>

    <td><%= stock_price.low %></td>

    <td><%= stock_price.close %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), stock_price, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_stock_price_path(stock_price), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), stock_price, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_stock_price_path, :title=>"Add a stock_price" %>
<% end %>
<%= content_for :footer do %>
<% end %>

