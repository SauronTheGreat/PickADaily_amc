<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing news_sectors</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Sector</th>

    <th>News</th>

    <th></th>
  </tr>

<% @news_sectors.each do |news_sector| %>
  <tr>

    <td><%= news_sector.sector_id %></td>

    <td><%= news_sector.news_id %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), news_sector, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_news_sector_path(news_sector), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), news_sector, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_news_sector_path, :title=>"Add a news_sector" %>
<% end %>
<%= content_for :footer do %>
<% end %>

