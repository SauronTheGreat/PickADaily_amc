<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing rounds</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Simulation</th>

    <th>Number</th>

    <th></th>
  </tr>

<% @rounds.each do |round| %>
  <tr>

    <td><%= round.simulation_id %></td>

    <td><%= round.number %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), round, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_round_path(round), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), round, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_round_path, :title=>"Add a round" %>
<% end %>
<%= content_for :footer do %>
<% end %>

