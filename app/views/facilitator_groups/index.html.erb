<%= content_for :head do %>
<script type="text/javascript">
    $(function() {
    });
</script>
<% end %>

<%= content_for :header do %>
<h1>Listing facilitator_groups</h1>
<% end %>

<%= content_for :main_content do %>
<table>
  <tr>

    <th>Name</th>

    <th>Facilitator</th>

    <th></th>
  </tr>

<% @facilitator_groups.each do |facilitator_group| %>
  <tr>

    <td><%= facilitator_group.name %></td>

    <td><%= facilitator_group.facilitator_id %></td>

    <td>
    <%= link_to image_tag(icon("show"),:size=>"64x64"), facilitator_group, :title=>'Show' %>
    <%= link_to image_tag(icon("edit"),:size=>"64x64"), edit_facilitator_group_path(facilitator_group), :title=>'Edit' %>
    <%= link_to image_tag(icon("delete"),:size=>"64x64"), facilitator_group, confirm: 'Are you sure?', method: :delete, :title=>'Delete' %>
    </td>
  </tr>
<% end %>
</table>
<% end %>

<br />
<%= content_for :navigation do %>
<% end %>
<%= content_for :extra do %>
<%= link_to image_tag('new_icon.png',:size=>"100x100"), new_facilitator_group_path, :title=>"Add a facilitator_group" %>
<% end %>
<%= content_for :footer do %>
<% end %>

